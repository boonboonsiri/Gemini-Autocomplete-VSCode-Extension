{
  "name": "gemini-autocomplete",
  "displayName": "Gemini Autocomplete",
  "description": "An Gemini AI autocomplete in vscode for how I want to use a vscode autocomplete extension",
  "version": "1.0.2",
  "publisher": "BoonBoonsiri",
  "repository": {
    "type": "git",
    "url": "https://github.com/boonboonsiri/Gemini-Autocomplete-VSCode-Extension.git"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:python"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gemini-autocomplete.activate",
        "title": "Activate Gemini Assistant"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0"
  }
}
